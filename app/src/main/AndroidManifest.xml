        <activity
            android:name="com.example.toshiba.restaurantapp.ListadoFinal"
            android:label="@string/title_activity_listado_final"
            tools:ignore="Instantiatable">
        </activity>
        <activity
            android:name="com.example.toshiba.restaurantapp.Notas"
            android:label="@string/title_activity_notas" >
