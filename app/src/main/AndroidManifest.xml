        <activity
            android:name="com.example.toshiba.restaurantapp.porciones"
            android:label="@string/title_activity_porciones" >
        </activity>
        <activity
            android:name="com.example.toshiba.restaurantapp.parrillada"
            android:label="@string/title_activity_parrillada" >
        </activity>
        <activity
            android:name="com.example.toshiba.restaurantapp.mariscos"
            android:label="@string/title_activity_mariscos" >
        </activity>
        <activity
            android:name="com.example.toshiba.restaurantapp.ControlMenu"
            android:label="@string/title_activity_control_menu" >
        </activity>
        <activity
            android:name="com.example.toshiba.restaurantapp.administrador"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
